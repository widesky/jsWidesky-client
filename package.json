{
  "name": "@widesky/jswidesky-client",
  "version": "2.0.1",
  "description": "WideSky Client in JavaScript",
  "main": "index.js",
  "private": false,
  "scripts": {
    "test": "JUNIT_REPORT_PATH=report.xml npx mocha --colors --reporter mocha-jenkins-reporter test/*/**.js",
    "coverage": "npx nyc --reporter=html --reporter=text mocha test/*/**.js",
    "build:bundlejs:prd": "webpack --config webpack.config.js --mode production",
    "build:bundlejs:dev": "webpack --config webpack.config.js --mode development",
    "build": "run-s test build:bundlejs:prd build:bundlejs:dev"
  },
  "author": "WideSky.cloud <support@widesky.com>",
  "license": "MIT",
  "dependencies": {
    "axios": "1.1.3",
    "bunyan": "^1.8.15",
    "form-data": "^4.0.0",
    "jsesc": "^3.0.2",
    "lodash": "^4.17.21",
    "moment-timezone": "^0.5.38"
  },
  "devDependencies": {
    "@babel/core": "^7.19.6",
    "@babel/preset-env": "^7.19.4",
    "babel-loader": "^9.0.1",
    "chai": "^4.3.6",
    "mocha": "^10.1.0",
    "mocha-jenkins-reporter": "^0.4.8",
    "npm-run-all": "^4.1.5",
    "nyc": "^15.1.0",
    "proxyquire": "^2.1.3",
    "sinon": "^14.0.1",
    "webpack": "^5.74.0",
    "webpack-cli": "^4.10.0",
    "webpack-dev-server": "^4.11.1"
  }
}
