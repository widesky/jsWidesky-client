{
  "name": "@widesky/jswidesky-client",
  "version": "1.1.2",
  "description": "WideSky Client in JavaScript",
  "main": "index.js",
  "private": false,
  "scripts": {
    "test": "JUNIT_REPORT_PATH=report.xml node node_modules/mocha/bin/mocha --colors --reporter mocha-jenkins-reporter test/*/**.js",
    "coverage": "node node_modules/nyc/bin/nyc.js --reporter=html --reporter=text node_modules/mocha/bin/mocha test/*/**.js",
    "build:bundlejs:prd": "webpack --config webpack.config.js --mode production",
    "build:bundlejs:dev": "webpack --config webpack.config.js --mode development",
    "build": "run-s test build:bundlejs:prd build:bundlejs:dev"
  },
  "author": "Widesky.cloud<support@widesky.com>",
  "license": "MIT",
  "dependencies": {
    "bluebird": "^3.5.2",
    "jsesc": "^2.5.1",
    "moment-timezone": "^0.5.31",
    "request": "^2.88.0",
    "request-promise": "^4.2.2"
  },
  "devDependencies": {
    "@babel/core": "^7.12.9",
    "@babel/preset-env": "^7.12.7",
    "babel-loader": "^8.2.2",
    "npm-run-all": "^4.1.5",
    "webpack": "^4.44.2",
    "webpack-cli": "^3.3.12",
    "webpack-dev-server": "^3.11.0",
    "chai": "^4.2.0",
    "mocha": "^5.2.0",
    "mocha-jenkins-reporter": "^0.4.5",
    "nyc": "^15.0.0",
    "sinon": "^7.1.0",
    "proxyquire": "^2.1.3"
  }
}
